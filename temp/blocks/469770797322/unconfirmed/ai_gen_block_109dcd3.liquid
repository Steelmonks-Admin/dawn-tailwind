bile and desktop seperately. Right now it seems to be working for desktop but not for mobile. The images are getting cut off on mobile, the formatting does not work yet for mobile. Please fix the issue so that the format on mobile also gets adapted by the first image uploaded to the mobile placeholder, on mobile, it uses the image from the desktop section and does not make it fit. Please fix this issue
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-slider-{{ ai_gen_id }} {
    display: block;
    position: relative;
    width: 100%;
    overflow: hidden;
  }

  .ai-slider-container-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
  }

  .ai-slider-track-{{ ai_gen_id }} {
    display: flex;
    transition: transform 0.5s ease-in-out;
  }

  .ai-slider-slide-{{ ai_gen_id }} {
    min-width: 100%;
    flex-shrink: 0;
    display: flex;
    flex-direction: column;
  }

  .ai-slider-image-wrapper-{{ ai_gen_id }} {
    width: 100%;
    position: relative;
    overflow: hidden;
  }

  .ai-slider-image-wrapper-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    display: block;
    object-fit: contain;
    object-position: center;
  }

  .ai-slider-image-desktop-{{ ai_gen_id }} {
    display: block;
  }

  .ai-slider-image-mobile-{{ ai_gen_id }} {
    display: none;
  }

  .ai-slider-image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .ai-slider-image-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
  }

  .ai-slider-text-container-{{ ai_gen_id }} {
    text-align: center;
    padding-top: {{ block.settings.text_padding_top_desktop }}px;
    padding-bottom: {{ block.settings.text_padding_bottom_desktop }}px;
    padding-left: {{ block.settings.text_padding_horizontal_desktop }}px;
    padding-right: {{ block.settings.text_padding_horizontal_desktop }}px;
  }

  .ai-slider-heading-{{ ai_gen_id }} {
    margin: 0 0 {{ block.settings.heading_subheading_gap_desktop }}px 0;
    font-size: {{ block.settings.heading_size_desktop }}px;
    font-weight: {{ block.settings.heading_weight }};
    color: {{ block.settings.heading_color }};
    line-height: 1.2;
  }

  .ai-slider-subheading-{{ ai_gen_id }} {
    margin: 0;
    font-size: {{ block.settings.subheading_size_desktop }}px;
    font-weight: {{ block.settings.subheading_weight }};
    color: {{ block.settings.subheading_color }};
    line-height: 1.4;
  }

  .ai-slider-arrow-{{ ai_gen_id }} {
    position: absolute;
    background-color: {{ block.settings.arrow_background_color }};
    color: {{ block.settings.arrow_icon_color }};
    border: none;
    width: {{ block.settings.arrow_size }}px;
    height: {{ block.settings.arrow_size }}px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 10;
    transition: opacity 0.3s ease;
    top: 0;
    transform: translateY(0);
  }

  .ai-slider-arrow-{{ ai_gen_id }}:hover {
    opacity: 0.8;
  }

  .ai-slider-arrow-prev-{{ ai_gen_id }} {
    left: {{ block.settings.arrow_offset }}px;
  }

  .ai-slider-arrow-next-{{ ai_gen_id }} {
    right: {{ block.settings.arrow_offset }}px;
  }

  .ai-slider-arrow-{{ ai_gen_id }} svg {
    width: calc({{ block.settings.arrow_size }}px * 0.5);
    height: calc({{ block.settings.arrow_size }}px * 0.5);
  }

  @media screen and (max-width: 749px) {
    .ai-slider-image-desktop-{{ ai_gen_id }} {
      display: none;
    }

    .ai-slider-image-mobile-{{ ai_gen_id }} {
      display: block;
    }

    .ai-slider-text-container-{{ ai_gen_id }} {
      padding-top: {{ block.settings.text_padding_top_mobile }}px;
      padding-bottom: {{ block.settings.text_padding_bottom_mobile }}px;
      padding-left: {{ block.settings.text_padding_horizontal_mobile }}px;
      padding-right: {{ block.settings.text_padding_horizontal_mobile }}px;
    }

    .ai-slider-heading-{{ ai_gen_id }} {
      margin-bottom: {{ block.settings.heading_subheading_gap_mobile }}px;
      font-size: {{ block.settings.heading_size_mobile }}px;
    }

    .ai-slider-subheading-{{ ai_gen_id }} {
      font-size: {{ block.settings.subheading_size_mobile }}px;
    }

    .ai-slider-arrow-{{ ai_gen_id }} {
      width: {{ block.settings.arrow_size_mobile }}px;
      height: {{ block.settings.arrow_size_mobile }}px;
    }

    .ai-slider-arrow-{{ ai_gen_id }} svg {
      width: calc({{ block.settings.arrow_size_mobile }}px * 0.5);
      height: calc({{ block.settings.arrow_size_mobile }}px * 0.5);
    }

    .ai-slider-arrow-prev-{{ ai_gen_id }} {
      left: {{ block.settings.arrow_offset_mobile }}px;
    }

    .ai-slider-arrow-next-{{ ai_gen_id }} {
      right: {{ block.settings.arrow_offset_mobile }}px;
    }
  }
{% endstyle %}

<image-slider-{{ ai_gen_id }}
  class="ai-slider-{{ ai_gen_id }}"
  data-autoplay="{{ block.settings.autoplay_enabled }}"
  data-autoplay-speed="{{ block.settings.autoplay_speed }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-slider-container-{{ ai_gen_id }}">
    <div class="ai-slider-track-{{ ai_gen_id }}">
      {% for i in (1..block.settings.slide_count) %}
        {% liquid
          assign desktop_image_key = 'slide_' | append: i | append: '_image_desktop'
          assign mobile_image_key = 'slide_' | append: i | append: '_image_mobile'
          assign heading_key = 'slide_' | append: i | append: '_heading'
          assign subheading_key = 'slide_' | append: i | append: '_subheading'

          assign desktop_image = block.settings[desktop_image_key]
          assign mobile_image = block.settings[mobile_image_key]
          assign heading = block.settings[heading_key]
          assign subheading = block.settings[subheading_key]
        %}

        <div class="ai-slider-slide-{{ ai_gen_id }}" data-slide-index="{{ forloop.index0 }}">
          <div class="ai-slider-image-wrapper-{{ ai_gen_id }}">
            {% if desktop_image or mobile_image %}
              {% if desktop_image %}
                <img
                  class="ai-slider-image-desktop-{{ ai_gen_id }}"
                  src="{{ desktop_image | image_url: width: 3000 }}"
                  alt="{{ desktop_image.alt | escape }}"
                  loading="{% if forloop.first %}eager{% else %}lazy{% endif %}"
                  width="{{ desktop_image.width }}"
                  height="{{ desktop_image.height }}"
                  data-desktop-width="{{ desktop_image.width }}"
                  data-desktop-height="{{ desktop_image.height }}"
                  {% if forloop.first %}
                    data-first-desktop="true"
                  {% endif %}
                >
              {% endif %}
              {% if mobile_image %}
                <img
                  class="ai-slider-image-mobile-{{ ai_gen_id }}"
                  src="{{ mobile_image | image_url: width: 1500 }}"
                  alt="{{ mobile_image.alt | escape }}"
                  loading="{% if forloop.first %}eager{% else %}lazy{% endif %}"
                  width="{{ mobile_image.width }}"
                  height="{{ mobile_image.height }}"
                  data-mobile-width="{{ mobile_image.width }}"
                  data-mobile-height="{{ mobile_image.height }}"
                  {% if forloop.first %}
                    data-first-mobile="true"
                  {% endif %}
                >
              {% elsif desktop_image %}
                <img
                  class="ai-slider-image-mobile-{{ ai_gen_id }}"
                  src="{{ desktop_image | image_url: width: 1500 }}"
                  alt="{{ desktop_image.alt | escape }}"
                  loading="{% if forloop.first %}eager{% else %}lazy{% endif %}"
                  width="{{ desktop_image.width }}"
                  height="{{ desktop_image.height }}"
                  data-mobile-width="{{ desktop_image.width }}"
                  data-mobile-height="{{ desktop_image.height }}"
                  {% if forloop.first and mobile_image == blank %}
                    data-first-mobile="true"
                  {% endif %}
                >
              {% endif %}
            {% else %}
              <div class="ai-slider-image-placeholder-{{ ai_gen_id }}">
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>

          <div class="ai-slider-text-container-{{ ai_gen_id }}">
            {% if heading != blank %}
              <h2 class="ai-slider-heading-{{ ai_gen_id }}">{{ heading }}</h2>
            {% endif %}
            {% if subheading != blank %}
              <p class="ai-slider-subheading-{{ ai_gen_id }}">{{ subheading }}</p>
            {% endif %}
          </div>
        </div>
      {% endfor %}
    </div>

    <button
      class="ai-slider-arrow-{{ ai_gen_id }} ai-slider-arrow-prev-{{ ai_gen_id }}"
      aria-label="Previous slide"
      type="button"
    >
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="15 18 9 12 15 6"></polyline>
      </svg>
    </button>

    <button
      class="ai-slider-arrow-{{ ai_gen_id }} ai-slider-arrow-next-{{ ai_gen_id }}"
      aria-label="Next slide"
      type="button"
    >
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="9 18 15 12 9 6"></polyline>
      </svg>
    </button>
  </div>
</image-slider-{{ ai_gen_id }}>

<script>
  (function() {
    class ImageSlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 0;
        this.autoplayInterval = null;
        this.isInitialized = false;
        this.resizeTimeout = null;
      }

      connectedCallback() {
        this.track = this.querySelector('.ai-slider-track-{{ ai_gen_id }}');
        this.slides = this.querySelectorAll('.ai-slider-slide-{{ ai_gen_id }}');
        this.prevButton = this.querySelector('.ai-slider-arrow-prev-{{ ai_gen_id }}');
        this.nextButton = this.querySelector('.ai-slider-arrow-next-{{ ai_gen_id }}');
        this.totalSlides = this.slides.length;

        this.autoplayEnabled = this.dataset.autoplay === 'true';
        this.autoplaySpeed = parseInt(this.dataset.autoplaySpeed) * 1000;

        this.setupEventListeners();
        this.initializeSlider();

        window.addEventListener('resize', () => {
          clearTimeout(this.resizeTimeout);
          this.resizeTimeout = setTimeout(() => {
            this.setSlideHeights();
            this.positionArrows();
          }, 100);
        });
      }

      initializeSlider() {
        const firstSlide = this.slides[0];
        if (!firstSlide) return;

        const isMobile = window.innerWidth <= 749;
        const firstImage = firstSlide.querySelector('[data-first-mobile="true"]');
        const firstDesktopImage = firstSlide.querySelector('[data-first-desktop="true"]');

        const imageToLoad = isMobile ? firstImage : firstDesktopImage;

        if (imageToLoad) {
          if (imageToLoad.complete && imageToLoad.naturalWidth > 0) {
            this.setSlideHeights();
            this.updateSlidePosition();
            this.positionArrows();
            this.isInitialized = true;
            
            if (this.autoplayEnabled && this.totalSlides > 1) {
              this.startAutoplay();
            }
          } else {
            imageToLoad.addEventListener('load', () => {
              this.setSlideHeights();
              this.updateSlidePosition();
              this.positionArrows();
              this.isInitialized = true;
              
              if (this.autoplayEnabled && this.totalSlides > 1) {
                this.startAutoplay();
              }
            });
          }
        } else {
          this.setSlideHeights();
          this.updateSlidePosition();
          this.positionArrows();
          this.isInitialized = true;
          
          if (this.autoplayEnabled && this.totalSlides > 1) {
            this.startAutoplay();
          }
        }
      }

      setSlideHeights() {
        const firstSlide = this.slides[0];
        if (!firstSlide) return;

        const isMobile = window.innerWidth <= 749;
        const firstImage = firstSlide.querySelector('[data-first-mobile="true"]');
        const firstDesktopImage = firstSlide.querySelector('[data-first-desktop="true"]');

        const imageToUse = isMobile ? firstImage : firstDesktopImage;
        
        if (!imageToUse) return;

        let aspectRatio;
        
        if (isMobile) {
          const mobileWidth = parseInt(imageToUse.getAttribute('data-mobile-width'));
          const mobileHeight = parseInt(imageToUse.getAttribute('data-mobile-height'));
          
          if (mobileWidth && mobileHeight && mobileWidth > 0 && mobileHeight > 0) {
            aspectRatio = mobileHeight / mobileWidth;
          } else if (imageToUse.naturalWidth > 0 && imageToUse.naturalHeight > 0) {
            aspectRatio = imageToUse.naturalHeight / imageToUse.naturalWidth;
          } else {
            return;
          }
        } else {
          const desktopWidth = parseInt(imageToUse.getAttribute('data-desktop-width'));
          const desktopHeight = parseInt(imageToUse.getAttribute('data-desktop-height'));
          
          if (desktopWidth && desktopHeight && desktopWidth > 0 && desktopHeight > 0) {
            aspectRatio = desktopHeight / desktopWidth;
          } else if (imageToUse.naturalWidth > 0 && imageToUse.naturalHeight > 0) {
            aspectRatio = imageToUse.naturalHeight / imageToUse.naturalWidth;
          } else {
            return;
          }
        }

        const containerWidth = this.offsetWidth;
        const calculatedHeight = containerWidth * aspectRatio;

        this.slides.forEach((slide) => {
          const imageWrapper = slide.querySelector('.ai-slider-image-wrapper-{{ ai_gen_id }}');
          if (imageWrapper) {
            imageWrapper.style.height = calculatedHeight + 'px';
          }
        });
      }

      setupEventListeners() {
        this.prevButton.addEventListener('click', () => {
          this.goToPrevSlide();
          if (this.autoplayEnabled) {
            this.resetAutoplay();
          }
        });

        this.nextButton.addEventListener('click', () => {
          this.goToNextSlide();
          if (this.autoplayEnabled) {
            this.resetAutoplay();
          }
        });
      }

      goToPrevSlide() {
        this.currentSlide = (this.currentSlide - 1 + this.totalSlides) % this.totalSlides;
        this.updateSlidePosition();
        this.positionArrows();
      }

      goToNextSlide() {
        this.currentSlide = (this.currentSlide + 1) % this.totalSlides;
        this.updateSlidePosition();
        this.positionArrows();
      }

      updateSlidePosition() {
        const offset = -this.currentSlide * 100;
        this.track.style.transform = 'translateX(' + offset + '%)';
      }

      positionArrows() {
        const currentSlideElement = this.slides[this.currentSlide];
        if (!currentSlideElement) return;

        const imageWrapper = currentSlideElement.querySelector('.ai-slider-image-wrapper-{{ ai_gen_id }}');
        if (!imageWrapper) return;

        const imageHeight = imageWrapper.offsetHeight;
        const arrowHeight = this.prevButton.offsetHeight;
        const centerPosition = (imageHeight - arrowHeight) / 2;

        this.prevButton.style.top = centerPosition + 'px';
        this.nextButton.style.top = centerPosition + 'px';
      }

      startAutoplay() {
        this.autoplayInterval = setInterval(() => {
          this.goToNextSlide();
        }, this.autoplaySpeed);
      }

      resetAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
          this.startAutoplay();
        }
      }

      disconnectedCallback() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
        }
        if (this.resizeTimeout) {
          clearTimeout(this.resizeTimeout);
        }
      }
    }

    customElements.define('image-slider-{{ ai_gen_id }}', ImageSlider{{ ai_gen_id }});
  })();
</script>

