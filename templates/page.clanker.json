ke);\n      background:\n        radial-gradient(120% 90% at 20% 0%, rgba(255,152,0,0.18) 0%, rgba(255,255,255,0) 46%),\n        radial-gradient(120% 90% at 80% 0%, rgba(0,68,204,0.18) 0%, rgba(255,255,255,0) 46%),\n        linear-gradient(180deg, var(--glass-dark) 0%, var(--glass-dark-2) 100%);\n      box-shadow: var(--shadow2);\n      overflow: hidden;\n      isolation: isolate;\n      text-decoration: none;\n      color: inherit;\n      display: grid;\n      gap: 12px;\n      padding: 16px 16px 14px;\n      transform: translateZ(0);\n      will-change: transform, box-shadow, filter;\n      backdrop-filter: blur(10px);\n      -webkit-backdrop-filter: blur(10px);\n      transition: transform 260ms cubic-bezier(0.2,0.8,0.2,1), box-shadow 260ms ease, filter 260ms ease;\n      transform-origin: center;\n      min-width: 0;\n    }\n\n    #smw-types .smwtypes__card:after{\n      content: \"\";\n      position: absolute;\n      inset: 0;\n      border-radius: var(--radius);\n      pointer-events: none;\n      background: linear-gradient(135deg, rgba(255,255,255,0.16), rgba(255,255,255,0.04));\n      opacity: 0.65;\n      z-index: 0;\n    }\n\n    #smw-types .smwtypes__card:before{\n      content: \"\";\n      position: absolute;\n      inset: -40px;\n      background:\n        radial-gradient(40% 38% at 30% 25%, rgba(255,152,0,0.22), rgba(255,255,255,0) 60%),\n        radial-gradient(44% 40% at 70% 30%, rgba(0,68,204,0.24), rgba(255,255,255,0) 62%),\n        radial-gradient(30% 30% at 50% 85%, rgba(255,255,255,0.10), rgba(255,255,255,0) 65%);\n      opacity: 0;\n      transition: opacity 260ms ease;\n      z-index: 0;\n      pointer-events: none;\n      filter: blur(2px);\n    }\n\n    #smw-types .smwtypes__card:hover{\n      transform: translateY(-4px) scale(var(--hover-scale));\n      box-shadow: var(--shadow3);\n      filter: saturate(1.02);\n    }\n    #smw-types .smwtypes__card:hover:before{ opacity: 1; }\n\n    #smw-types .smwtypes__card:focus-visible{\n      outli