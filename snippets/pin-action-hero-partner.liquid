<div class="pin-action-hero__partner" {{ block.shopify_attributes }}>
  {% if block.settings.partner_logo != blank %}
    <a
      href="{{ block.settings.partner_url }}"
      target="_blank"
      class="pin-action-hero__partner-link"
    >
      {{
        block.settings.partner_logo
        | image_url: width: 90
        | image_tag: class: 'pin-action-hero__partner-image', loading: 'lazy', width: 100, height: 100
      }}
    </a>
  {% endif %}
</div>
